<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>cin tree</Title>
      <Shortcut>cintree</Shortcut>
      <Description>cin for tree</Description>
      <Author>Yohta Kimura id:rajyan</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
        <!--<SnippetType>SurroundsWith</SnippetType>-->
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>V</ID>
          <Default>N</Default>
        </Literal>
      </Declarations>
      <Code Language="cpp">
        <![CDATA[
vector<vector<int>> edges($V$);
for (int i = 0; i < $V$ - 1; i++) {
  int u, v;
	cin >> u >> v;
  u--, v--;
	edges[u].emplace_back(v);
	edges[v].emplace_back(u);
}$end$]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>

<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>coutyesno</Title>
      <Shortcut>yesno</Shortcut>
      <Description>cout yes no \n</Description>
      <Author>Yohta Kimura id:rajyan</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
        <!--<SnippetType>SurroundsWith</SnippetType>-->
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>flag</ID>
          <Default>flag</Default>
          <ToolTip>bool flag</ToolTip>
        </Literal>
      </Declarations>
      <Code Language="cpp">
        <![CDATA[if($flag$) cout << "Yes" << "\n";
        else cout << "No" << "\n";$end$]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>